{
  "name": "gtfs-to-geojson",
  "version": "3.4.1",
  "private": false,
  "description": "Generate geoJSON of transit route data from a GTFS file",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "geojson"
  ],
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-geojson/issues"
  },
  "repository": "git://github.com/blinktaginc/gtfs-to-geojson",
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [
    "Kegan Maher"
  ],
  "type": "module",
  "main": "index.js",
  "bin": {
    "gtfs-to-geojson": "bin/gtfs-to-geojson.js"
  },
  "scripts": {
    "lint": "eslint **/*.js --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@turf/bbox": "^6.5.0",
    "@turf/bbox-polygon": "^6.5.0",
    "@turf/buffer": "^6.5.0",
    "@turf/convex": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/meta": "^6.5.0",
    "@turf/simplify": "^6.5.0",
    "@turf/union": "^6.5.0",
    "archiver": "^5.3.1",
    "cli-table": "^0.3.11",
    "gtfs": "^4.4.0",
    "lodash-es": "^4.17.21",
    "pretty-error": "^4.0.0",
    "sanitize-filename": "^1.6.3",
    "timer-machine": "^1.1.0",
    "yargs": "^17.7.2",
    "yoctocolors": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-config-xo": "^0.43.1",
    "husky": "^8.0.3",
    "pretty-quick": "^3.1.3"
  },
  "engines": {
    "node": ">= 12.14.0"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
